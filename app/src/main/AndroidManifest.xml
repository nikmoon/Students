<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nikbird.students">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityLists" />
        <activity
            android:name=".ActivityStudents"
            android:launchMode="singleTask" />
        <activity
            android:name=".ActivityGroups"
            android:launchMode="singleTask" />
        <activity android:name=".ActivityRegistration" />
        <activity android:name=".ActivityStudentProfile" />
        <activity android:name=".ActivityLessons"></activity>
    </application>

</manifest>